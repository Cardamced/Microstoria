{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "**/*" // à l'origine le fichier contenait "src/**/* Modif suite au déplacement du dossier types à la racine du projet" 
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}